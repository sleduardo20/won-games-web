{
  "name": "won-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --fix --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNoNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -s ./public -p 6008",
    "build-storybook": "build-storybook -s ./public"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.14.0",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/fa-brands": "^10.26.0",
    "@styled-icons/material-outlined": "^10.28.0",
    "axios": "^0.21.1",
    "build": "^0.1.4",
    "dotenv": "^9.0.0",
    "graphql": "^15.5.0",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "10.2.0",
    "next-auth": "^3.23.0",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "nextjs-progressbar": "^0.0.9",
    "polished": "^4.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.2.3",
    "styled-icons": "^10.33.0",
    "styled-media-query": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-typescript": "^7.12.13",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/lodash.xor": "^4.5.6",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@typescript-eslint/typescript-estree": "^4.22.0",
    "apollo": "^2.32.13",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.0",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "msw": "^0.28.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.0.5",
    "pretty-format": "^26.6.0",
    "react-is": "^17.0.2",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
